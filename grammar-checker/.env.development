# Development Environment Variables
VITE_API_URL=http://localhost:3001
VITE_APP_NAME=AI Grammar Checker (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_ENVIRONMENT=development

# Auth0 Configuration (development values)
VITE_AUTH0_DOMAIN=your-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-development-client-id
VITE_AUTH0_AUDIENCE=your-development-api-audience

# Sentry Configuration (development values)
VITE_SENTRY_DSN=your-development-sentry-dsn
VITE_SENTRY_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=false