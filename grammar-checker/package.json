{
  "name": "grammar-checker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production tsc -b && vite build --mode production",
    "build:vercel": "tsc -b && vite build --mode production",
    "build:analyze": "npm run build:prod && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "type-check": "tsc -b --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint && npm run type-check",
    "prebuild:prod": "npm run clean && npm run lint && npm run type-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "playwright test",
    "test:integration:ui": "playwright test --ui",
    "test:integration:headed": "playwright test --headed",
    "test:integration:debug": "playwright test --debug",
    "test:integration:report": "playwright show-report",
    "test:all": "npm run test && npm run test:integration",
    "test:continuous": "node scripts/continuous-testing.js",
    "test:watch:all": "concurrently \"npm run test:watch\" \"npm run test:integration -- --watch\"",
    "test:ci": "npm run test:coverage && npm run test:integration"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.4.0",
    "@sentry/react": "^10.5.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.85.9",
    "@types/react-router-dom": "^5.3.3",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-swipeable": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/diff": "^7.0.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^2.1.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "diff": "^8.0.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-html-reporter": "^4.3.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.12",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-bundle-analyzer": "^0.11.0"
  }
}
